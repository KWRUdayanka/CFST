<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #65CD03;" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="demo.controller.HomeScreenController">
    <children>
        <Label layoutX="129.0" layoutY="43.0" text="Let's Save Our Mother Nature!" textFill="WHITE">
            <font>
                <Font name="System Bold" size="24.0" />
            </font>
        </Label>
        <Button fx:id="viewMyProfileButton" layoutX="128.0" layoutY="89.0" mnemonicParsing="false" onAction="#onViewMyProfile" style="-fx-background-color: #979797;" text="View My Profile" textFill="WHITE" />
        <Button fx:id="addDeviceInformationButton" layoutX="107.0" layoutY="147.0" mnemonicParsing="false" onAction="#onAddDeviceInformation" style="-fx-background-color: #979797;" text="Add Device Information" textFill="WHITE" />
        <Button fx:id="calculateMyCFButton" layoutX="370.0" layoutY="147.0" mnemonicParsing="false" onAction="#calculateMyCF" style="-fx-background-color: #979797;" text="Calculate My CF" textFill="WHITE" />
