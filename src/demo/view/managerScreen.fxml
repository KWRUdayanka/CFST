      <Label layoutX="128.0" layoutY="8.0" textAlignment="CENTER" textFill="WHITE">
         <font>
            <Font name="System Bold" size="24.0" />
         </font>
      </Label>
      <Label fx:id="welcomeMessage" layoutX="126.0" layoutY="14.0" prefHeight="35.0" prefWidth="351.0" textAlignment="CENTER" textFill="WHITE">
         <font>
            <Font name="System Bold" size="24.0" />
         </font>
      </Label>
    </children>
</AnchorPane>
