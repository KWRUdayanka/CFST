<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.shape.Line?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #65CD03;" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="demo.controller.LoginController">
   <children>
      <Label alignment="CENTER" layoutX="278.0" layoutY="63.0" text="Login" textAlignment="CENTER" textFill="WHITE">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Label>
      <Line endX="100.0" layoutX="300.0" layoutY="54.0" startX="-100.0" stroke="WHITE" />
      <Line endX="100.0" layoutX="300.0" layoutY="102.0" startX="-100.0" stroke="WHITE" />
      <TextField fx:id="emailFiled" layoutX="200.0" layoutY="118.0" prefHeight="25.0" prefWidth="201.0" promptText="CQU Email" />
      <Button fx:id="loginButton" layoutX="218.0" layoutY="218.0" mnemonicParsing="false" onAction="#onLogin" prefHeight="25.0" prefWidth="60.0" style="-fx-background-color: #979797;" text="Login" textFill="WHITE" />
      <Button fx:id="registerButton" layoutX="323.0" layoutY="218.0" mnemonicParsing="false" onAction="#onRegister" style="-fx-background-color: #979797;" text="Register" textFill="WHITE" />
      <Label fx:id="userLoggingLabel" alignment="CENTER" layoutX="145.0" layoutY="267.0" prefHeight="17.0" prefWidth="311.0" textAlignment="CENTER" textFill="RED" />
      <PasswordField fx:id="passwordField" layoutX="200.0" layoutY="166.0" prefHeight="25.0" prefWidth="201.0" promptText="Password" />
   </children>
</AnchorPane>
